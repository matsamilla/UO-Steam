if not @findobject 'Chest' and not inrange 'Chest' 2
  promptalias 'Chest'
endif
if not @findobject 'RegBag'
  headmsg 'Select a bag for Regs'
  promptalias 'RegBag'
endif
if not @findobject 'ArmorBag'
  headmsg 'Select a bag for Armor'
  promptalias 'ArmorBag'
endif
